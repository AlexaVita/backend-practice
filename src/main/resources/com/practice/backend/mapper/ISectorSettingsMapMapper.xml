<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.practice.backend.mapper.ISectorSettingsMapMapper">

    <select id="getAll">
        select * from sectorsettingsmap
    </select>

    <select id="getById">
        select * from sectorsettingsmap where id = (#{id})
    </select>

    <insert id="insert" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO sectorsettingsmap(sector_id,name,value,payment,binlist,guappay) VALUES(#{sectorId},#{name},#{value},#{payment},#{binList},#{guapPay});
    </insert>

    <update id="update" parameterType="com.practice.backend.model.SectorSettingsMap">
        UPDATE sectorsettingsmap
        SET sector_id = #{sectorId},
            name = #{name},
            value = #{value},
            payment = #{payment},
            binlist = #{binList},
            guappay = #{guapPay}
        WHERE id = #{id};
    </update>

    <delete id="delete">
        delete from sectorsettingsmap where id = #{id}
    </delete>
</mapper>